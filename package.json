{
	"name": "front",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"mailhog": "mailhog -auth-file mailhog/auth -outgoing-smtp mailhog/outgoing.json",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"pushdb": "prisma db push && prisma generate",
		"configure:dev": "cp .env.dev .env",
		"configure:prod": "cp .env.prod .env",
		"pushdb:all": "pnpm configure:dev && pnpm pushdb && pnpm configure:prod && pnpm pushdb && pnpm configure:dev",
		"format": "prettier --plugin-search-dir . --write .",
		"start": "node -r dotenv/config build",
		"start:beta": "STAGE=beta ORIGIN=https://loca7.ewen.works pnpm start",
		"check-prisma-imports": "for file in **/*.server.ts; do if ! grep -q 'import { prisma } from' $file; then echo warn: no prisma import in $file; fi; done",
		"update-mail-templates": "ts-node mail-templates/updateTypes.ts",
		"update-icons": "python src/lib/icons/sveltify.py",
		"generate": "concurrently npm:update-mail-templates npm:update-icons && pnpm format"
	},
	"prisma": {
		"seed": "ts-node --transpile-only scripts/importOldData.ts"
	},
	"pre-commit": [
		"format",
		""
	],
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.0",
		"@types/bcrypt": "^5.0.0",
		"@types/cli-progress": "^3.11.0",
		"@types/lodash": "^4.14.192",
		"@types/md5": "^2.3.2",
		"@types/mime-types": "^2.1.1",
		"@types/mjml": "^4.7.1",
		"@types/nodemailer": "^6.4.7",
		"@types/uuid": "^9.0.1",
		"@types/zxcvbn": "^4.4.1",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"concurrently": "^7.6.0",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"html-to-text": "^9.0.5",
		"mime2ext": "^2.0.0",
		"nqdm": "^2.0.2",
		"password": "^0.1.1",
		"pre-commit": "^1.2.2",
		"prettier": "^2.8.7",
		"prettier-plugin-prisma": "^4.12.0",
		"prettier-plugin-svelte": "^2.10.0",
		"prisma": "^4.12.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.1.4",
		"sveltekit-superforms": "^0.6.6",
		"ts-node": "^10.9.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.2.1",
		"vitest": "^0.25.8",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@lexical/clipboard": "^0.8.1",
		"@lexical/history": "^0.8.1",
		"@lexical/html": "^0.8.1",
		"@lexical/link": "^0.8.1",
		"@lexical/list": "^0.8.1",
		"@lexical/markdown": "^0.8.1",
		"@lexical/rich-text": "^0.8.1",
		"@lexical/selection": "^0.8.1",
		"@lexical/utils": "^0.8.1",
		"@lucia-auth/adapter-prisma": "^0.4.0",
		"@lucia-auth/sveltekit": "^0.6.11",
		"@prisma/client": "^4.12.0",
		"autolinker": "^4.0.0",
		"bbcode.js": "^0.4.0",
		"bcrypt": "^5.1.0",
		"dialog-polyfill": "^0.5.6",
		"dotenv": "^16.0.3",
		"fuse.js": "^6.6.2",
		"handlebars": "^4.7.7",
		"ics": "^3.1.0",
		"lexical": "^0.8.1",
		"lodash-es": "^4.17.21",
		"lucia-auth": "^0.7.3",
		"md5": "^2.3.0",
		"mime-types": "^2.1.35",
		"mjml": "^4.13.0",
		"node-htmldiff": "^0.9.4",
		"nodemailer": "^6.9.1",
		"sharp": "^0.31.3",
		"slugify": "^1.6.6",
		"svelte-local-storage-store": "^0.4.0",
		"svelte-tiny-virtual-list": "^2.0.5",
		"sveltejs-tippy": "^3.0.0",
		"uuid": "^9.0.0",
		"vcard4": "^3.1.5",
		"xss": "^1.0.14",
		"zxcvbn": "^4.4.2"
	}
}
